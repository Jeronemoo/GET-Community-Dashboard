<%- include ("partials/header") %>
            <div class="container-fluid">
                <div class="row">
                    <div class="col-xl-4 col-sm-6 mb-xl-0 mb-4">
                        <div class="card">
                            <div class="card-body p-3">
                                <div class="row">
                                    <div class="col-8">
                                        <div class="numbers">
                                            <p class="text-sm mb-0 text-capitalize font-weight-bold">Total Tickets Sold</p>
                                            <h5 class="font-weight-bolder mb-0">
                                                <%= helpers.numberWithCommas(locals.ticketSales)%><br />
                                                    <span class="text-sm font-weight-bolder"><a href="#">NFT Explorer</a></span>
                                            </h5>
                                        </div>
                                    </div>
                                    <div class="col-4 text-end">
                                        <div class="icon icon-shape getGreen text-center border-radius-md">
                                            <i class="icon-ticket ni ni-money-coins text-lg opacity-10" aria-hidden="true"></i>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-xl-4 col-sm-6 mb-xl-0 mb-4">
                        <div class="card">
                            <div class="card-body p-3">
                                <div class="row">
                                    <div class="col-8">
                                        <div class="numbers">
                                            <p class="text-sm mb-0 text-capitalize font-weight-bold">Current Price</p>
                                            <h5 class="font-weight-bolder mb-0">
                                                <%= `$${locals.tokenData.tokenPriceUSD} | €${locals.tokenData.tokenPriceEUR}` %><br />
                                                    <span class="text-sm font-weight-bolder"><%= locals.tokenData.priceChange24h.toFixed(2) %>%</span>
                                            </h5>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-xl-4 col-sm-6 mb-xl-0 mb-4">
                        <div class="card">
                            <div class="card-body p-3">
                                <div class="row">
                                    <div class="col-8">
                                        <div class="numbers">
                                            <p class="text-sm mb-0 text-capitalize font-weight-bold">Market Cap</p>
                                            <h5 class="font-weight-bolder mb-0">
                                                <%= `$${locals.tokenData.marketCapUSD}` %><br />
                                                    <span class="text-sm"><%= `€${locals.tokenData.marketCapEUR}` %></span>
                                            </h5>
                                        </div>
                                    </div>
                                    <div class="col-4 text-end">
                                        <div class="icon icon-shape getGreen text-center border-radius-md">
                                            <i class="icon-chart-line ni ni-paper-diploma text-lg opacity-10" aria-hidden="true"></i>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>

                </div>
                <!-- end row -->
                <div class="row mt-4">
                    <div class="col-lg-6 col-md-6 mb-md-0 mb-4">
                        <div class="card">
                            <div class="card-header pb-0">
                                <div class="row">
                                    <div class="col-lg-6 col-7">
                                        <h6>Top 10 Events</h6>
                                        <p class="text-sm mb-0">
                                            <span class="font-weight-bold ms-1">by GET used</span>
                                        </p>
                                    </div>
                                </div>
                            </div>
                            <div class="card-body px-0 pb-2">
                                <div class="table-responsive">
                                    <table class="table align-items-center mb-0">
                                        <thead>
                                            <tr>
                                                <th class="text-secondary text-xxs">Event Name</th>
                                                <th class="text-secondary text-xxs">Ticketeer Name</th>
                                                <th class="text-secondary text-xxs">Mint Count (tickets)</th>
                                                <th class="text-secondary text-xxs">GET Used</th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                            <% for(var i = 0; i < locals.topEvents.length; i++) { %>
                                                <tr>
                                                    <td>
                                                        <div class="d-flex px-2 py-1">
                                                            <a href="event-profile/<%= locals.topEvents[i].id%>">
                                                                <%= locals.topEvents[i].eventName %>
                                                            </a>
                                                        </div>
                                                    </td>
                                                    <td>
                                                        <div class="d-flex px-2 py-1">
                                                            <h6 class="mb-0 text-sm">
                                                                <a href="../ticketeer/<%= locals.topEvents[i].ticketeerName %>">
                                                                    <%= locals.topEvents[i].ticketeerName %>
                                                                </a>
                                                            </h6>
                                                        </div>
                                                    </td>
                                                    <td>
                                                        <div class="d-flex px-2 py-1">
                                                            <h6 class="mb-0 text-sm">
                                                                <%= locals.helpers.numberWithCommas(locals.topEvents[i].mintCount) %>
                                                            </h6>
                                                        </div>
                                                    </td>
                                                    <td>
                                                        <div class="d-flex px-2 py-1">
                                                            <h6 class="mb-0 text-sm">
                                                                <%= Number(locals.topEvents[i].getDebitedFromSilo).toFixed(6) %>
                                                            </h6>
                                                        </div>
                                                    </td>
                                                </tr>
                                                <% } %>
                                        </tbody>
                                    </table>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-lg-6 col-md-6 mb-md-0 mb-4">
                        <div class="card">
                            <div class="card-header pb-0">
                                <div class="row">
                                    <div class="col-lg-6 col-7">
                                        <h6>Recent Events</h6>
                                    </div>
                                </div>
                            </div>
                            <div class="card-body px-0 pb-2">
                                <div class="table-responsive">
                                    <table class="table align-items-center mb-0">
                                        <thead>
                                            <tr>
                                                <th class="text-secondary text-xxs">Block Timestamp</th>
                                                <th class="text-secondary text-xxs">Event Name</th>
                                                <th class="text-secondary text-xxs">Ticketeer</th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                            <% for(var i = 0; i < locals.recentEvents.length; i++) { %>
                                                <tr>
                                                    <td>
                                                        <div class="d-flex px-2 py-1">
                                                            <h6 class="mb-0 text-sm">
                                                                <%= locals.helpers.dateR(locals.recentEvents[i].blockTimestamp) %>
                                                            </h6>
                                                        </div>
                                                    </td>
                                                    <td>
                                                        <div class="d-flex px-2 py-1">
                                                            <a href="event-profile/<%= locals.recentEvents[i].event.id%>">
                                                                <%= locals.recentEvents[i].event.eventName %>
                                                            </a>
                                                        </div>
                                                    </td>
                                                    <td>
                                                        <div class="d-flex px-2 py-1">
                                                            <h6 class="mb-0 text-sm">
                                                                <a href="../ticketeer/<%= locals.recentEvents[i].event.ticketeerName %>">
                                                                    <%= locals.recentEvents[i].event.ticketeerName %>
                                                                </a>
                                                            </h6>
                                                        </div>
                                                    </td>
                                                </tr>
                                                <% } %>
                                        </tbody>
                                    </table>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
    </div>

    <%- include ("partials/footer") %>